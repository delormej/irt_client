{
  "name": "irt-client",
  "description": "Test client for ANT+ Inside Ride rollers.",
  "publisher": "IRT, LLC",
  "author": {
    "email": "jason@insideride.com",
    "name": "Jason De Lorme",
    "url": "http://www.insideride.com"
  },
  "version": "2.5.6",
  "main": "./src/main.js",
  "license": "MIT",
  "repository": "irt_client",
  "scripts": {
    "start": "electron-forge start",
    "prestart": "npm run build",
    "postinstall": "./node_modules/.bin/electron-rebuild",
    "build": "tsc --project ./src/tsconfig.json",
    "clean": "rmdir ./src/node_modules /s /q",
    "package": "electron-forge package",
    "make": "electron-forge make"
  },
  "dependencies": {
    "@types/react": "^16.0.36",
    "asar": "^0.14.0",
    "babel-preset-react": "^6.3.13",
    "babel-register": "^6.3.13",
    "electron-compile": "^6.4.2",
    "electron-settings": "^3.1.4",
    "electron-squirrel-startup": "^1.0.0",
    "ffi": "^2.2.0",
    "process": "^0.11.10",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "ref": "^1.3.5"
  },
  "devDependencies": {
    "babel-plugin-transform-async-to-generator": "^6.24.1",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "electron-forge": "^4.2.0",
    "electron-prebuilt-compile": "1.7.11",
    "electron-rebuild": "^1.6.0",
    "@amcharts/amcharts3-react": "^3.0.0",
    "react-scripts": "1.0.14"
  },
  "config": {
    "forge": {
      "make_targets": {
        "win32": [
          "zip",
          "squirrel"
        ],
        "darwin": [
          "zip"
        ],
        "linux": [
          "deb",
          "rpm"
        ]
      },
      "electronPackagerConfig": {
        "packageManager": "npm",
        "icon": "src/images/irt.ico"
      },
      "electronWinstallerConfig": {
        "name": "irt_test_client"
      },
      "electronInstallerDebian": {},
      "electronInstallerRedhat": {},
      "github_repository": {
        "owner": "",
        "name": ""
      },
      "windowsStoreConfig": {
        "packageName": "",
        "name": "irttestclient"
      }
    }
  }
}
