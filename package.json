{
  "name": "irt-client",
  "description": "Test client for ANT+ Inside Ride rollers.",
  "publisher": "IRT, LLC",
  "author": {
    "email": "jason@insideride.com",
    "name": "Jason De Lorme",
    "url": "http://www.insideride.com"
  },
  "version": "3.0.0",
  "main": "./src/main.js",
  "license": "MIT",
  "repository": "irt_client",
  "scripts": {
    "start": "electron-forge start",
    "prestart": "npm run build",
    "postinstall": "./node_modules/.bin/electron-rebuild",
    "build": "tsc --project ./src/tsconfig.json",
    "clean": "rmdir ./src/node_modules /s /q",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "debug": "./node_modules/.bin/electron-forge-vscode-win.cmd --remote-debugging-port=9222"
  },
  "dependencies": {
    "@amcharts/amcharts3-react": "^3.0.0",
    "@types/react": "^16.0.40",
    "ant-plus": "file:../ant-plus/ant-plus-0.2.5.tgz",
    "azure-storage": "^2.10.2",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.3.13",
    "babel-register": "^6.3.13",
    "electron-settings": "^3.1.4",
    "electron-squirrel-startup": "^1.0.0",
    "process": "^0.11.10",
    "react": "^16.14.0",
    "react-dom": "^16.14.0"
  },
  "devDependencies": {
    "@electron-forge/cli": "^6.0.0-beta.54",
    "@electron-forge/maker-appx": "^6.0.0-beta.54",
    "@electron-forge/maker-deb": "^6.0.0-beta.54",
    "@electron-forge/maker-rpm": "^6.0.0-beta.54",
    "@electron-forge/maker-squirrel": "^6.0.0-beta.54",
    "@electron-forge/maker-zip": "^6.0.0-beta.54",
    "@electron-forge/plugin-compile": "^6.0.0-beta.54",
    "@electron-forge/publisher-github": "^6.0.0-beta.54",
    "@types/node": "^14.14.20",
    "babel-plugin-transform-async-to-generator": "^6.24.1",
    "babel-plugin-transform-es2015-destructuring": "^6.23.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "electron": "^12.0.2",
    "electron-rebuild": "^2.3.5",
    "react-scripts": "^4.0.3"
  },
  "config": {
    "forge": {
      "packagerConfig": {},
      "makers": [
        {
          "name": "@electron-forge/maker-squirrel",
          "config": {
            "name": "irt_client"
          }
        },
        {
          "name": "@electron-forge/maker-zip",
          "platforms": [
            "darwin"
          ]
        },
        {
          "name": "@electron-forge/maker-deb",
          "config": {}
        },
        {
          "name": "@electron-forge/maker-rpm",
          "config": {}
        }
      ]
    },
    "electronPackagerConfig": {
      "packageManager": "npm",
      "icon": "src/images/irt.ico"
    },
    "electronWinstallerConfig": {
      "name": "irt_test_client"
    },
    "electronInstallerDebian": {},
    "electronInstallerRedhat": {},
    "github_repository": {
      "owner": "",
      "name": ""
    },
    "windowsStoreConfig": {
      "packageName": "",
      "name": "irttestclient"
    }
  }
}
